<%- include('../parts/html-header')  %> 
<%- include('../parts/navbar') %> 

<div class="container">
    <div class="row">
        <div class="col">
            <table class="table table-striped">
                <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">姓名</th>
                <th scope="col">郵件</th>
                <th scope="col">手機</th>
                <th scope="col">生日</th>
                <th scope="col">地址</th>
              </tr>
            </thead>
            <tbody>
                <% for(let i of rows){ %> 
              <tr>
                  <td><%= i.sid %> </td>
                  <td><%= i.name %> </td>
                  <td><%= i.email %> </td>
                  <td><%= i.mobile %> </td>
                  <td><%=toDateString(i.birthday) %> </td>
                  <td><%= i.address %> </td>  
              </tr>
              <% } %> 
            
            </tbody>
          
            </table>
        </div>
    </div>
</div>

<%- include('../parts/script') %> 
<%- include('../parts/html-footer') %> 